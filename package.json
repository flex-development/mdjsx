    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-tree-shaking": "^1.8.0",
    "husky": "^4.3.0",
    "jest": "^27.0.0-next.2",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.2",
